{
  "name": "sistema-gerenciamento-usuarios",
  "version": "2.0.0",
  "description": "Sistema de gerenciamento de usuários com validação de CPF integrada",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "demo": "node src/index.js --demo",
    "transpile": "mkdir -p dist && cp -r src/* dist/ || exit 0",
    "build": "npm run clean && npm run transpile && npm install && echo 'Build concluído!'",
    "build:main": "npm run clean && npm install --production && ./build.sh",
    "send-email": "node scripts/send_email.js",
    "clean": "rm -rf node_modules build && mkdir build",
    "dev": "npx nodemon src/index.js",
    "test": "jest --runInBand --reporters=default --reporters=jest-junit --outputFile=junit.xml"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "moment": "^2.29.4",
    "nodemailer": "^7.0.6"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "nodemon": "^3.0.1",
    "jest-junit": "^16.0.0"
  },
  "keywords": [
    "gerenciamento",
    "usuarios",
    "cpf",
    "validacao",
    "nodejs",
    "cli"
  ],
  "author": "Equipe de Engenharia de Software",
  "license": "ISC"
}